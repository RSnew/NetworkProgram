<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.model.dao.QueryDao">
    <select id="findAll" 	resultType="com.project.model.pojo.Query"  >
        select * from ssmbighomework.answertable
    </select>
    <update id="addQuery" parameterType="com.project.model.pojo.Query">
        insert into ssmbighomework.answertable(answerStudentID,answerQuestion) values
            (#{answerStudentID}, #{answerQuestion});
    </update>
</mapper>